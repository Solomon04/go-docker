{
  "name": "go-docker",
  "version": "1.0.0",
  "description": "Go Lang + GraphQL API",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.7.6",
    "husky": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
